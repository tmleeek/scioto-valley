/* website Switcher */
.storeswitch {display: inline-block;}
#websiteSwitcherOverlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #000;
    filter:alpha(opacity=30);
    -moz-opacity:0.3;
    -khtml-opacity: 0.3;
    opacity: 0.3;
    z-index: 200;
}
a.websiteSwitcher {
    background: url("../images/locations.png") 10px 50% no-repeat;
	cursor: pointer;
    position: relative;
    display: inline-block;
    vertical-align: middle;
    color: #E31837;
    line-height: 24px;
    height: 24px;
    padding: 6px 0 0.75em 30px;
}
a.websiteSwitcher span {
    display: block;
    color: #E31837;
    line-height: 24px;
    height: 24px;
    padding: 0 5px;
}
a.websiteSwitcher:hover {
    background-color: #f5f5f5;
}
a.websiteSwitcher:hover > span {
    color: #000;
}
a.websiteSwitcher div.hoverOver { 
    display: none; 
    position: absolute;
    top: -3px;
    right: -11px;
    z-index: 999;
    padding:0;
    -webkit-box-shadow: 6px 8px 5px 0px rgba(50, 50, 50, 0.75);
    -moz-box-shadow:    6px 8px 5px 0px rgba(50, 50, 50, 0.75);
    box-shadow:         6px 8px 5px 0px rgba(50, 50, 50, 0.75);
}
a.websiteSwitcher div.hoverOver span {
    background: #fff url("../images/locations.png") 15px 10px no-repeat;
    display: block;
    float: right;
    color: #E31837;
    line-height: 24px;
    height: 24px;
    padding: 0.75em 16px 5px 40px;
}
a.websiteSwitcher div.hoverOver .hoverBox {
    clear: right;
    padding: 20px 31px;
    background-color:#DCDDDF;
    text-align: left;
}
/*a.websiteSwitcher:hover div.hoverOver { display: block; }*/
a.websiteSwitcher select { width: 200px; }
a.websiteSwitcher label { display: block; margin-bottom: 6px; font-size: 0.8em; color: #e31837 !important; }
a.websiteSwitcher .locationButtons {
	margin: 6px 0 0 0;
}
a.websiteSwitcher .locationButtons button {
	color: #e31837 !important;
	border: 1px solid #DCDDDF;
	background: #fff;
	font-weight: normal;
	font-size: 0.8em;
}
a.websiteSwitcher .locationButtons button.leftButton { margin-right: 24px;}


